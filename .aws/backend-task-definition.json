{
  "containerDefinitions": [
    {
      "name": "booklink-backend-contr",
      "image": "booklink-backend:latest",
      "essential": true,
      "memory": "256",
      "cpu": "128",
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "8080",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:703057871606:secret:booklink-access-key-id-71wgVM"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:703057871606:secret:booklink-secret-access-key-UawS8p"
        }
       ],
      "environment": [
        {
          "name": "APP_ENV",
          "value": "live"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-central-1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-booklink-app",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "awslogs-booklink-be"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "volumes": [],
  "networkMode": "bridge",
  "memory": "384",
  "cpu": "512",
  "placementConstraints": [],
  "family": "booklink-backend-ec2-taskdef",
  "tags": [
    {
      "key": "booklink",
      "value": "backend"
    }
  ]
}