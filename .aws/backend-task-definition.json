{
  "containerDefinitions": [
    {
      "name": "booklink-backend-contr",
      "image": "booklink-backend:latest",
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "8080",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:703057871606:secret:booklink-access-key-id-71wgVM"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:703057871606:secret:booklink-secret-access-key-UawS8p"
        }
       ],
      "environment": [
        {
          "name": "TITLE",
          "value": "Booklink"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-central-1"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "volumes": [],
  "networkMode": "bridge",
  "memory": "1024",
  "cpu": "0.5 vCPU",
  "placementConstraints": [],
  "family": "booklink-backend-ec2-taskdef",
  "tags": [
    {
      "key": "booklink-foo",
      "value": "bar"
    }
  ]
}