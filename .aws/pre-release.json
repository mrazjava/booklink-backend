{
  "containerDefinitions": [
    {
      "name": "booklink-backend-contr",
      "image": "DYNAMICALLY_REPLACED_BY_WORKFLOW:pre-release.yml",
      "essential": true,
      "healthCheck": {
        "command": [ "CMD-SHELL", "curl -f http://localhost:8080/actuator/health || exit 1" ],
        "interval": "10",
        "timeout": "5",
        "startPeriod": "45",
        "retries": "2"
      },
      "memory": "512",
      "memoryReservation": "256",
      "cpu": "512",
      "portMappings": [
        {
          "hostPort": "8888",
          "containerPort": "8080",
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "APP_ENV",
          "value": "pre"
        },
        {
          "name": "CORS_ALLOW_ORIGINS",
          "value": "http://ec2-3-124-3-167.eu-central-1.compute.amazonaws.com, http://pre.booklinktrove.com, https://pre.booklinktrove.com"
        },
        {
          "name": "APP_BASE_URL",
          "value": "http://ec2-3-124-3-167.eu-central-1.compute.amazonaws.com:8888"
        },
        {
          "name": "APP_HIKARI_CON_TIMEOUT",
          "value": "4000"
        },
        {
          "name": "APP_BE_DB_URL",
          "value": "jdbc-secretsmanager:postgresql://pre-1.cnzxipfrf9ok.eu-central-1.rds.amazonaws.com:5432/booklink"
        },
        {
          "name": "APP_SPRING_DATA_DRIVER",
          "value": "com.amazonaws.secretsmanager.sql.AWSSecretsManagerPostgreSQLDriver"
        },
        {
          "name": "APP_BE_DB_USERNAME",
          "value": "pre1/postgres"
        },
        {
          "name": "APP_BE_HIBERNATE_DDL_AUTO",
          "value": "validate"
        },
        {
          "name": "APP_SPRING_DATA_INIT",
          "value": "never"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-central-1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ec2/pre/booklink-backend",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "pre"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "executionRoleArn": "BooklinkEcsTaskExecutionRole",
  "volumes": [],
  "networkMode": "bridge",
  "memory": "768",
  "cpu": "768",
  "placementConstraints": [],
  "family": "booklink-backend-ec2-taskdef",
  "tags": [
    {
      "key": "booklink",
      "value": "backend"
    }
  ]
}