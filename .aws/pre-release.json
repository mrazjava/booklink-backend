{
  "containerDefinitions": [
    {
      "name": "booklink-backend-contr",
      "image": "DYNAMICALLY_REPLACED_BY_WORKFLOW:pre-release.yml",
      "essential": true,
      "memory": "256",
      "cpu": "256",
      "portMappings": [
        {
          "hostPort": "8888",
          "containerPort": "8080",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:703057871606:secret:booklink-access-key-id-71wgVM"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:703057871606:secret:booklink-secret-access-key-UawS8p"
        }
       ],
      "environment": [
        {
          "name": "APP_ENV",
          "value": "pre"
        },
        {
          "name": "CORS_ALLOW_ORIGINS",
          "value": "http://ec2-3-124-3-167.eu-central-1.compute.amazonaws.com"
        },
        {
          "name": "APP_BASE_URL",
          "value": "http://ec2-3-124-3-167.eu-central-1.compute.amazonaws.com:8888"
        },
        {
          "name": "APP_HIKARI_CON_TIMEOUT",
          "value": "0"
        },
        {
          "name": "APP_BE_DB_URL",
          "value": "jdbc-secretsmanager:postgresql://pre-1.cnzxipfrf9ok.eu-central-1.rds.amazonaws.com:5432/booklink"
        },
        {
          "name": "APP_SPRING_DATA_DRIVER",
          "value": "com.amazonaws.secretsmanager.sql.AWSSecretsManagerPostgreSQLDriver"
        },
        {
          "name": "APP_BE_DB_USERNAME",
          "value": "pre1/postgres"
        },
        {
          "name": "APP_BE_HIBERNATE_DDL_AUTO",
          "value": "validate"
        },
        {
          "name": "APP_SPRING_DATA_INIT",
          "value": "never"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-central-1"
        },
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "eu-central-1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-booklink-backend-pre",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "pre"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "volumes": [],
  "networkMode": "bridge",
  "memory": "384",
  "cpu": "512",
  "placementConstraints": [],
  "family": "booklink-backend-ec2-taskdef",
  "tags": [
    {
      "key": "booklink",
      "value": "backend"
    }
  ]
}