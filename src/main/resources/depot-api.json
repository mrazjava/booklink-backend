    "/rest/v1/depot/author/flex/{id}": {
      "get": {
        "tags": [
          "Author"
        ],
        "summary": "Find author matching a specific ID optionally exlude image(s)",
        "operationId": "findByIdUsingGET",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "unique author ID",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "imgS",
            "in": "query",
            "description": "fetch small img?",
            "required": false,
            "style": "form",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "imgM",
            "in": "query",
            "description": "fetch medium img?",
            "required": false,
            "style": "form",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "imgL",
            "in": "query",
            "description": "fetch large img?",
            "required": false,
            "style": "form",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "ok",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/DepotAuthor"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
